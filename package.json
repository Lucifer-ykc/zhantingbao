{
  "name": "zhantingbao",
  "version": "1.0.0",
  "private": true,
  "description": "zhantingbao",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "less"
  },
  "scripts": {
    "start": "npm run build:weapp -- --watch",
    "starth5": "npm run build:h5 -- --watch",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/components": "2.2.3",
    "@tarojs/components-qa": "2.2.3",
    "@tarojs/router": "2.2.3",
    "@tarojs/taro": "2.2.3",
    "@tarojs/taro-alipay": "2.2.3",
    "@tarojs/taro-h5": "2.2.3",
    "@tarojs/taro-qq": "2.2.3",
    "@tarojs/taro-quickapp": "2.2.3",
    "@tarojs/taro-swan": "2.2.3",
    "@tarojs/taro-tt": "2.2.3",
    "@tarojs/taro-weapp": "2.2.3",
    "@tarojs/taro-rn": "2.2.3",
    "babel-runtime": "^6.26.0",
    "regenerator-runtime": "0.11.1",
    "nervjs": "^1.5.5",
    "nerv-devtools": "^1.5.5"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/webpack-env": "^1.13.6",
    "@tarojs/mini-runner": "2.2.3",
    "@tarojs/webpack-runner": "2.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-jsx-stylesheet": "^0.6.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-eslint": "^8.2.3",
    "eslint": "^5.16.0",
    "eslint-config-taro": "2.2.3",
    "eslint-plugin-react": "^7.8.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-import": "^2.12.0",
    "stylelint": "9.3.0",
    "stylelint-config-taro-rn": "2.2.3",
    "stylelint-taro-rn": "2.2.3",
    "eslint-plugin-taro": "2.2.3"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "no-mixed-spaces-and-tabs": [
        2,
        false
      ],
      "react/jsx-filename-extension": 0,
      "no-debugger": 0,
      "no-console": 0,
      "jsx-a11y/alt-text": 0,
      "max-len": 0,
      "react/prop-types": 0,
      "eqeqeq": 0,
      "no-bitwise": 0,
      "comma-dangle": "off",
      "react/jsx-no-bind": "off",
      "react/no-array-index-key": 0,
      "func-names": 0,
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "no-undef": 1,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "arrow-parens": 0,
      "import/no-extraneous-dependencies": 0,
      "global-require": 0,
      "import/order": 0,
      "prefer-arrow-callback": 0,
      "no-useless-return": 0,
      "quotes": 0,
      "consistent-return": 0,
      "import/no-dynamic-require": 0,
      "import/no-unresolved": 0,
      "react/destructuring-assignment": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-closing-bracket-location": 0,
      "import/prefer-default-export": 0,
      "lines-between-class-members": 0,
      "react/jsx-wrap-multilines": 0,
      "import/named": 0,
      "object-property-newline": 0,
      "object-curly-newline": 0,
      "import/no-mutable-exports": 0,
      "guard-for-in": 0,
      "no-restricted-syntax": 0,
      "camelcase": 0,
      "react/jsx-indent": 0,
      "no-nested-ternary": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "no-shadow": 0,
      "no-return-assign": 0,
      "no-unused-expressions": 0,
      "no-useless-escape": 0,
      "react/sort-comp": 0,
      "prefer-template": 0,
      "react/no-unescaped-entities": 0,
      "no-alert": 0,
      "default-case": 0,
      "no-plusplus": 0,
      "no-mixed-operators": 0,
      "no-continue": 0,
      "jsx-a11y/label-has-for": 0,
      "react/button-has-type": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "import/no-cycle": 0,
      "react/no-multi-comp": 0,
      "jsx-a11y/label-has-associated-control": 0,
      "react/no-did-update-set-state": 0,
      "no-underscore-dangle": 0,
      "semi": [
        "error",
        "always"
      ],
      "no-multi-assign": 0,
      "prefer-rest-params": 0,
      "react-hooks/rules-of-hooks": "error",
      "react/react-in-jsx-scope":"off",
      "no-fallthrough": 0
    }
  }
}
